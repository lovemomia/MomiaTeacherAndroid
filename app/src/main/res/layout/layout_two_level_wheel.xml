<?xml version="1.0" encoding="utf-8"?>
<com.youxing.sogoteacher.views.TwoLevelWheelView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white">

    <com.aigestudio.wheelpicker.view.WheelCurvedPicker
        android:id="@+id/wheel_view_left"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1" />

    <com.aigestudio.wheelpicker.view.WheelCurvedPicker
        android:id="@+id/wheel_view_right"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1" />


</com.youxing.sogoteacher.views.TwoLevelWheelView>